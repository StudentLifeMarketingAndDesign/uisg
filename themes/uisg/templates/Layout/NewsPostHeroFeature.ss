<% if $FeaturedInitiative %>
	<% if $LatestNewsPosts %>
		<% with $LatestNewsPosts.First %>
			<div class="hero-article clearfix">
			    <% if $FeaturedImage %>
			    		<a href="$Link" target="_blank"><img src="data:image/png;base64,"  data-src="$FeaturedImage.ScaleWidth(250).URL" role="presentation" alt=""></a>
			    <% end_if %>
			    <h3 class="hero-title">
			        <a href="$Link">$Title</a>
			    </h3>
			    <div class="hero-content">$Content.Summary(30)</div>
			    <a href="$Link" class="hero-link">Read More</a>
			</div>
		<% end_with %>
	<% end_if %>
<% else %>
	<% loop $LatestNewsPosts %>
	<div class="hero-article clearfix">
	    <% if $FeaturedImage %>
	    		<a href="$Link" target="_blank"><img src="data:image/png;base64,"  data-src="$FeaturedImage.ScaleWidth(250).URL" role="presentation" alt=""></a>
	    <% end_if %>
	    <h3 class="hero-title">
	        <a href="$Link">$Title</a>
	    </h3>
	    <div class="hero-content">$Content.Summary(30)</div>
	    <a href="$Link" class="hero-link">Read More</a>
	</div>
	<% end_loop %>

<% end_if %>